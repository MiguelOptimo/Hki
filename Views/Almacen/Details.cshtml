@using hki.web.Models.Identity
@using Microsoft.AspNetCore.Identity
@model Orden
@inject UserManager<ApplicationUser> _user

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>H&KI - Almacen</title>

    <!-- Bootstrap -->
    <link href="~/Board/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Board/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Board/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Board/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
	
    <!-- bootstrap-progressbar -->
    <link href="~/Board/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Board/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet"/>
    <!-- bootstrap-daterangepicker -->
    <link href="~/Board/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="~/Board/build/css/custom.min.css" rel="stylesheet">
  </head>

  <body class="nav-md">
    <div class="container body">
    <div class="main_container">
    <div class="col-md-3 left_col">
      <div class="left_col scroll-view">

        <div class="clearfix"></div>

        <!-- menu profile quick info -->
        <div class="profile clearfix">
          <div class="profile_pic">
            <img src="~/Board/production/images/img.jpg" alt="..." class="img-circle profile_img">
          </div>
          <div class="profile_info">
            <span>Welcome,</span>
            <h2>@_user.GetUserName(User)</h2>
          </div>
        </div>
        <!-- /menu profile quick info -->

        <br />

        <!-- sidebar menu -->
        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
          <div class="menu_section">
            <h3>Almacen</h3>
            <ul class="nav side-menu">
              <!-- Inicio -->
              <li>
                <a asp-controller="Almacen" asp-action="Index" ><i class="fa fa-home"></i> Home </a>
              </li>
                  
              <li>
                <a asp-controller="Almacen" asp-action="Mos" ><i class="fa fa-edit"></i> MO'S </a>
              </li>
              <li><a><i class="fa fa-bar-chart-o"></i> Reportes <span class="fa fa-chevron-down"></span></a>
                <ul class="nav child_menu">
                  <li><a href="#">Reporte de unidades por surtir, surtidas y completas</a></li>
                  <li><a href="#">Reporte de tiempo transcurrido de surtir a preparado</a></li>
                </ul>
              </li>
            </ul>
          </div>
        </div>
        <!-- /sidebar menu -->

        <!-- /menu footer buttons -->
        <div class="sidebar-footer hidden-small">
          <a data-toggle="tooltip" data-placement="top" title="Settings">
            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
          </a>
          <a data-toggle="tooltip" data-placement="top" title="FullScreen">
            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
          </a>
          <a data-toggle="tooltip" data-placement="top" title="Lock">
            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
          </a>
          <a data-toggle="tooltip" data-placement="top" title="Logout" href="#">
            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
          </a>
        </div>
        <!-- /menu footer buttons -->
      </div>
    </div>

    <!-- top navigation -->
    <div class="top_nav">
      <div class="nav_menu">
        <nav>
          <div class="nav toggle">
            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
          </div>

          <ul class="nav navbar-nav navbar-right">
            <li class="">
              <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                <img src="~/Board/production/images/img.jpg" alt=""> @_user.GetUserName(User)
                <span class=" fa fa-angle-down"></span>
              </a>
              <ul class="dropdown-menu dropdown-usermenu pull-right">
                <li><a href="javascript:;"> Profile</a></li>
                <li><a asp-action="Logut"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
              </ul>
            </li>

            <li role="presentation" class="dropdown">
              <a href="javascript:;" class="dropdown-toggle info-number" data-toggle="dropdown" aria-expanded="false">
                <i class="fa fa-envelope-o"></i>
                <span class="badge bg-green">6</span>
              </a>
              <ul id="menu1" class="dropdown-menu list-unstyled msg_list" role="menu">
                <li>
                  <a>
                    <span class="image"><img src="~/Board/production/images/img.jpg" alt="Profile Image" /></span>
                    <span>
                      <span>John Smith</span>
                      <span class="time">3 mins ago</span>
                    </span>
                    <span class="message">
                      Film festivals used to be do-or-die moments for movie makers. They were where...
                    </span>
                  </a>
                </li>
                <li>
                  <a>
                    <span class="image"><img src="~/Board/production/images/img.jpg" alt="Profile Image" /></span>
                    <span>
                      <span>John Smith</span>
                      <span class="time">3 mins ago</span>
                    </span>
                    <span class="message">
                      Film festivals used to be do-or-die moments for movie makers. They were where...
                    </span>
                  </a>
                </li>
                <li>
                  <a>
                    <span class="image"><img src="~/Board/production/images/img.jpg" alt="Profile Image" /></span>
                    <span>
                      <span>John Smith</span>
                      <span class="time">3 mins ago</span>
                    </span>
                    <span class="message">
                      Film festivals used to be do-or-die moments for movie makers. They were where...
                    </span>
                  </a>
                </li>
                <li>
                  <a>
                    <span class="image"><img src="~/Board/production/images/img.jpg" alt="Profile Image" /></span>
                    <span>
                      <span>John Smith</span>
                      <span class="time">3 mins ago</span>
                    </span>
                    <span class="message">
                      Film festivals used to be do-or-die moments for movie makers. They were where...
                    </span>
                  </a>
                </li>
                <li>
                  <div class="text-center">
                    <a>
                      <strong>See All Alerts</strong>
                      <i class="fa fa-angle-right"></i>
                    </a>
                  </div>
                </li>
              </ul>
            </li>
          </ul>
        </nav>
      </div>
    </div>
    <!-- /top navigation -->

    <!-- page content -->
    <div class="right_col" role="main">

      <!-- /Graph -->
      <div class="row">

                       <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2> Details </h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                      <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                          <li><a href="#">Settings 1</a>
                          </li>
                          <li><a href="#">Settings 2</a>
                          </li>
                        </ul>
                      </li>
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>

                  <div class="x_content">
             
                    <dl class="dl-horizontal">
                      <dt>
                        ID
                      </dt>

                      <dd>
                        @Model.Id
                      </dd>

                      <dt>
                        Asignado
                      </dt>

                      <dd>
                        @Model.Asignado
                      </dd>

                      <dt>
                        Fecha Levantamiento
                      </dt>

                      <dd>
                        @Model.Levantamiento
                      </dd>
                      <dt>
                        Fecha Requerimiento
                      </dt>

                      <dd>
                        @Model.FechaReq
                      </dd>
                      <dt>
                        Dia
                      </dt>
                      <dd>
                        @Model.Dia
                      </dd>
                      <dt>
                        Producto
                      </dt>
                      <dd>
                        <table class="table">
                          <tr>
                            <th>ID</th>
                            <th>Descripcion</th>
                            <th>Valor/Hrs</th>
                          </tr>
                          <tr>
                            <td>
                              @Model.ProductoId
                            </td>
                            <td>
                              @Model.Descripcion
                            </td>
                            <td>
                              @Model.ValorHrs
                            </td>
                          </tr>

                        </table>
                      </dd>
                      <dt>
                        Ubicacion
                      </dt>
                      <dd>
                        @Model.Ubicacion
                      </dd>
                      <dt>
                        Cantidad
                      </dt>
                      <dd>
                        @Model.Cantidad Piezas
                      </dd>
                      <dt>
                        Piezas finalizadas
                      </dt>
                      <dd>
                        @Model.Finalizadas Piezas
                      </dd>
                      <dt>
                        Valor Hrs.
                      </dt>
                      <dd>
                        @Model.ValorHrs
                      </dd>
                      <dt>
                        Total Hrs.
                      </dt>
                      <dd>
                        @Model.TotalHrs
                      </dd>
                      <dt>
                        Estatus 2
                      </dt>
                      <dd>
                        @Model.Estatus2
                      </dd>
                      <dt>
                        Estatus 3
                      </dt>
                      <dd>
                        @Model.Estatus3
                      </dd>
                    </dl>
							
                    <a asp-action="Mos">Regresar</a>
						
                  </div>
                </div>
              </div>
        
        
      </div>

            </div>
          </div>
        </div>
       
        <!-- /page content -->

        <!-- footer content -->
        <footer>
          <div class="pull-right">
            H&K International
          </div>
          <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->


    <!-- jQuery -->
    <script src="~/Board/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Board/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Board/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Board/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="~/Board/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="~/Board/vendors/gauge.js/dist/gauge.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="~/Board/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="~/Board/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="~/Board/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/Board/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Board/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Board/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Board/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Board/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="~/Board/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/Board/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/Board/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="~/Board/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="~/Board/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="~/Board/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="~/Board/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/Board/vendors/moment/min/moment.min.js"></script>
    <script src="~/Board/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="~/Board/build/js/custom.min.js"></script>
	
  </body>
</html>
